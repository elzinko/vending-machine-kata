{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "rootDir": ".",
    "outDir": "build",
    "types": ["node","jest"],
    "lib": [
      "es2017"
    ],
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts",
  ],
  "exclude": ["node_modules"]
}
